package org.kh.java;

public class Operation2 {

	public static void main(String[] args) {
		//산술연산
		
		int su1 = 98;
		int su2 = 18;
		System.out.println("su1 + su2 = "+ (su1 +su2));
		System.out.println("su1 - su2 = "+ (su1 -su2));
		System.out.println("su1 * su2 = "+ (su1 *su2));
		System.out.println("su1 / su2 = "+ (su1 /su2));
		System.out.println("su1 % su2 = "+ (su1 %su2));
		
		
		
		
		
		
		int a = 10;
		double b = 2.0;
		
		System.out.println((int)a+(int)b);
		System.out.println((int)a-b);
		System.out.println((int)a/b);
		System.out.println((int)a*b);
		
		char n = '글';
		System.out.println((int)n);
	}

}
